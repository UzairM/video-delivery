PORT=3001
API_DOMAIN=http://localhost:3001
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key
AWS_SECRET_ACCESS_KEY=your_secret_key
S3_BUCKET_NAME=your_bucket_name
CLOUDFRONT_DOMAIN=your_cloudfront_domain

# Processing
MAX_FILE_SIZE=104857600 # 100MB in bytes

# Video type settings:
# Use "all" to accept all video types, or specify comma-separated MIME types
# Example: ALLOWED_VIDEO_TYPES=all
# Example: ALLOWED_VIDEO_TYPES=video/mp4,video/quicktime

# Available video formats if not using "all":
# - video/mp4 (.mp4, .m4v)
# - video/quicktime (.mov)
# - video/x-msvideo (.avi)
# - video/x-matroska (.mkv)
# - video/webm (.webm)
# - video/x-ms-wmv (.wmv)
# - video/x-flv (.flv)
# - video/3gpp (.3gp)
# - video/x-m4v (.m4v)
# - video/mpeg (.mpeg, .mpg)
ALLOWED_VIDEO_TYPES=all 